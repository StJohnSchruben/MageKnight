<UserControl x:Class="MKView.Views.ArcControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MKView.Views"
             mc:Ignorable="d"  >
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <x:Static Member="local:SharedResourceDictionary.Instance" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <local:SpeedBoundary />
        <local:RangeSector />
        <Grid.RenderTransform>
            <RotateTransform CenterX="{Binding Model, Converter={StaticResource HighStatCenterOffsetToDouble}}" x:Name="Angle" CenterY="{Binding Model, Converter={StaticResource HighStatCenterOffsetToDouble}}" Angle="0"  />
        </Grid.RenderTransform>
    </Grid>
</UserControl>